import java.util.*;
public class Main {
    public static void main(String args[]) {
		int num,count,f,n; 
		Scanner sc = new Scanner(System.in);
	count=0;
    num = sc.nextInt(); 
    f=sc.nextInt(); 
	while(num!=0)
	{
		n=num%10; 
		if(n==f){
			count=count+1;
		}
        num=num/10;
	}
    System.out.println(count);
    }
}
