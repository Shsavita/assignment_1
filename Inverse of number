 import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int sum=0;
        int pos=1;
        while(num>0){
            int x= num%10;

            int m=pos*(int)(Math.pow(10,x-1));
            sum=sum +m;
            pos++;
            num=num/10;

        }
        System.out.print(sum);
    }
}
