import java.util.*;
public class Main {
    public static void main(String args[]) {
		Scanner sc= new Scanner(System.in);
		int n=sc.nextInt();
	 
		 System.out.println(fun(n));
	}
	
		public static int fun(int num){
				int sum=0;
		int i=0;
			while(num>0){
				int k= num%10;
				sum+= k*Math.pow(2,i);
				i++;
				num=num/10;
			}
			return sum;

		}  
}
