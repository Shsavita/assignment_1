import java.util.*;
public class Main {
    public static void main(String args[]) {
        // Your Code Here
		int i,j,n;
		Scanner sc = new Scanner(System.in);
	n = sc.nextInt();
	int no = 1;


	for(i=1;i<=n;i++){
		for ( j=1;j<=i;j++){
			System.out.print(no + "\t");
            no++;
		}
		System.out.println();
	}
    }
}
