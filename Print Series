import java.util.*;
public class Main {
    public static void main(String args[]) {
		Scanner sc=new Scanner(System.in);
		int N1=sc.nextInt();
		int N2=sc.nextInt();
		int c=1;
		int i=1;

		while(c<=N1){

			int k=3*i+2;
			if(k%N2!=0){
				System.out.println(k);
				c++;
			 
			}
			i++;
		}

    }
}
